      <div class="row my-1 py-3">
        <div class="col-md-3"></div>
        <div class="col-md-6">
          <h1 class="text-gray-dark text-center">REGISTER</h1>
          <p class="lead mb-4 text-center">Enter your details below to become a part of GoSpark</p>
          <form class="" method="post" action="/users/register">
            <div class="form-group"><label for="exampleInputEmail1">First Name</label>
              <div class="input-group">
                <div class="input-group-prepend"><span class="input-group-text" id="basic-addon1"><i class="fa fa-lg mt-1 fa-user"></i></span></div>
                <input type="text" name="first_name" class="form-control" id="inlineFormInputGroup" placeholder="First Name"> </div>
            </div>
            <div class="form-group"><label for="exampleInputEmail1">Last Name</label>
              <div class="input-group">
                <div class="input-group-prepend"><span class="input-group-text" id="basic-addon1"><i class="fa fa-lg mt-1 fa-user"></i></span></div>
                <input type="text" name="last_name" class="form-control" id="inlineFormInputGroup" placeholder="Last Name"> </div>
            </div>
            <div class="form-group"><label for="exampleInputEmail1">Email</label>
              <div class="input-group">
                <div class="input-group-prepend"><span class="input-group-text" id="basic-addon1"><i class="fa fa-lg mt-1 fa-envelope"></i></span></div>
                <input type="email" name="email" class="form-control" id="inlineFormInputGroup" placeholder="Email"> </div>
            </div>
            <div class="form-group"><label for="exampleInputEmail1">Password</label>
              <div class="input-group">
                <div class="input-group-prepend"><span class="input-group-text" id="basic-addon1"><i class="fa fa-lg mt-1 fa-key"></i></span></div>
                <input type="password" name="password" class="form-control" id="inlineFormInputGroup" placeholder="Password"> </div>
            </div>
            <div class="form-group"><label for="exampleInputEmail1">Confirm Password</label>
              <div class="input-group">
                <div class="input-group-prepend"><span class="input-group-text" id="basic-addon1"><i class="fa fa-lg mt-1 fa-key"></i></span></div>
                <input type="password" class="form-control" id="inlineFormInputGroup" placeholder="Confirm Password"> </div>
            </div>
            <div class="form-group"><label for="exampleInputEmail1">Gender</label>
              <div class="input-group">
                <div class="input-group-prepend"><span class="input-group-text" id="basic-addon1"><i class="fa fa-lg mt-1 fa-key"></i></span></div>
                <select class="" name="gender">
                  <option value="Male">Male</option>
                  <option value="Gender">Gender</option>
                </select> </div>
            </div>
            <div class="form-group"><label for="exampleInputEmail1">Date of Birth</label>
              <div class="input-group">
                <div class="input-group-prepend"><span class="input-group-text" id="basic-addon1"><i class="fa fa-lg mt-1 fa-calendar"></i></span></div>
                <input type="date" name="date_of_birth" class="form-control" id="inlineFormInputGroup" placeholder="Date of Birth"> </div>
            </div>
            <button type="submit" class="btn btn-primary btn-lg btn-block text-center w-75 px-0 m-5">Send</button>
          </form>
        </div>
      </div>
